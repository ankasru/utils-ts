    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "default": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc && eslint . --ext ts && vite build ",
